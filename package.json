{
  "name": "constituency",
  "version": "1.0.0",
  "description": "An overview of Congress",
  "main": "public/index.html",
  "dependencies": {
    "choo": "^4.0.3",
    "choo-log": "^3.0.0",
    "cookie-signature": "^1.0.6",
    "deep-equal": "^1.0.1",
    "icepick": "^1.3.0",
    "level": "^1.5.0",
    "localforage": "^1.4.3",
    "memdb": "^1.3.1",
    "merry": "^4.7.2",
    "node-static": "^0.7.9",
    "request": "^2.79.0",
    "run-series": "^1.1.4",
    "tachyons": "^4.6.2",
    "uuid": "^3.0.1",
    "xhr": "^2.3.3"
  },
  "devDependencies": {
    "browserify": "^13.3.0",
    "budo": "^9.4.5",
    "concurrently": "^3.1.0",
    "envify": "^4.0.0",
    "es2020": "^1.1.9",
    "nodemon": "^1.11.0",
    "pino-colada": "^1.0.3",
    "sheetify": "^6.0.1",
    "tape": "^4.6.3",
    "uglify-js": "^2.7.5",
    "uglifyify": "^3.0.4",
    "xo": "^0.17.1"
  },
  "scripts": {
    "start-prod": "NODE_ENV=production concurrently \"browserify -g uglifyify client/main.js -o public/main.bundle.js\" \"node server\"",
    "start-dev": "NODE_ENV=development concurrently \"budo client/main.js:main.bundle.js --pushstate --live --dir public\" \"nodemon --ignore client/ ./server.js | pino-colada\"",
    "test": "xo && NODE_ENV=test tape api/test/**/*.js && NODE_ENV=test budo test/main.js"
  },
  "browserify": {
    "transform": [
      "envify",
      "sheetify/transform",
      "es2020"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abradley2/constituency"
  },
  "author": "Tony Bradley",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/abradley2/constituency/issues"
  },
  "homepage": "https://github.com/abradley2/constituency#readme",
  "xo": {
    "envs": [
      "node",
      "commonjs",
      "browser"
    ],
    "semicolon": false
  }
}
